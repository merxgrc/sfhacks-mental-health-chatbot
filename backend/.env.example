# Google API Key for Gemini
GOOGLE_API_KEY=your_google_api_key_here

# Database Provider - Choose either "mongodb" or "chromadb"
DB_PROVIDER=mongodb

# MongoDB Atlas Connection URI (only needed if DB_PROVIDER=mongodb)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# ChromaDB path (only needed if DB_PROVIDER=chromadb)
CHROMADB_PATH=./chroma_db